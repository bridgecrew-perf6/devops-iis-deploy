{
    "manifestVersion": 1,
    "id": "devops-iis-deploy",
    "name": "Devops IIS Deploy",
    "version": "1.0.15",
    "publisher": "encoders",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Task to deploy IIS web/apps with full configuration transformation.",
    "categories": [
        "Azure Pipelines"
    ],
    "branding": {
        "color": "rgb(19, 99, 34)",
        "theme": "light"
    },
    "public": true,
    "tags": [
        "pipelines",
        "deploy",
        "transformation"
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "icons": {
        "default": "images/extension-icon.png"
    },
    "files": [
        {
            "path": "dist"
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/ronicarvalho/devops-iis-deploy"
    },
    "contributions": [
        {
            "id": "devops-iis-deploy",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "dist"
            }
        }
    ]
}